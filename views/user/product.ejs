<!-- Add to cart Sweet popup -->


<script>
    document.getElementById('add-to-cart-form').addEventListener('submit', function(e) {
        e.preventDefault(); // Prevent the form from submitting immediately

        // Show SweetAlert when the form is submitted
        Swal.fire({
            icon: 'success',
            title: 'Added to Cart',
            text: 'Your item has been added to the cart!',
        }).then((result) => {
            // After the popup closes, submit the form programmatically
            if (result.isConfirmed) {
                this.submit(); // Submit the form after the user closes the popup
            }
        });
    });
</script>




<!-- SweetAlert Script -->
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script>
    document.addEventListener('DOMContentLoaded', function() {
        const quantityInput = document.getElementById('quantity-input');
        const stock = <%= product.stock %>; // Get the stock value from the backend
        const maxLimit = Math.min(5, stock); // Set the max limit based on stock or 5
        
        // Set the maximum value for the input
        quantityInput.max = maxLimit;

        // If the user tries to increase the quantity beyond the limit, reset it to the max limit and show SweetAlert
        quantityInput.addEventListener('input', function() {
            if (parseInt(quantityInput.value) > maxLimit) {
                // Reset the input value to the maximum allowed quantity
                quantityInput.value = maxLimit;
                
                // Show SweetAlert message
                Swal.fire({
                    title: 'Oops!',
                    text: `You can only add up to ${maxLimit} items.`,
                    icon: 'warning',
                    confirmButtonText: 'Okay'
                });
            }
        });
    });
</script>





<!-- Zooming Script Section -->


<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script src="https://cdn.rawgit.com/elevateweb/elevatezoom/master/jquery.elevateZoom.min.js"></script>

<script>
  $(document).ready(function () {
    // Initialize ElevateZoom on the main image
    function initializeZoom() {
      $('#product-zoom').elevateZoom({
        zoomType: "lens",          // Type of zoom
        lensShape: "round",        // Shape of the zoom lens
        lensSize: 250,             // Size of the zoom lens
        scrollZoom: true,          // Enable scroll zoom
        easing: true,              // Apply easing effect for transitions
        responsive: true           // Make it responsive
      });
    }

    // Initialize zoom for the main product image
    initializeZoom();  // Initialize zoom for the first image

    // Event listener for thumbnail click
    $('.product-gallery-item').on('click', function (e) {
      e.preventDefault();
      
      const newImage = $(this).attr('data-image');            // Get new image path
      const newZoomImage = $(this).attr('data-zoom-image');   // Get new zoom image path

      // Ensure both image paths are correct
      if (!newImage || !newZoomImage) {
        console.error("Invalid image paths:", newImage, newZoomImage);
        return;
      }

      // Remove previous zoom instance and zoom container
      $('#product-zoom').removeData('elevateZoom');
      $('.zoomContainer').remove();  // Remove zoom container

      // Update the main image and zoom image
      $('#product-zoom')
        .attr('src', newImage)  // Update the main image source
        .attr('data-zoom-image', newZoomImage);  // Update the zoom image

      // Reinitialize zoom functionality
      initializeZoom();  // Re-initialize zoom for the new image
    });
  });
</script>




